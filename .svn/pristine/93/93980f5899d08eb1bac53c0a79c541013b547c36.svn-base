package com.thd.dataaccess.util;

import java.util.List;

public class ListUtil {
	public static Object getOne(List<Object> l ){
		if( (l!=null) && !l.isEmpty()){
			if(l.get(0)!=null){
				return l.get(0);
			}else{
				return null;
			}			
		}else{
			return null;
		}
	}
	
	public static boolean isNotEmpty(List l){
		return (l!=null && !l.isEmpty());
	}
	
}
